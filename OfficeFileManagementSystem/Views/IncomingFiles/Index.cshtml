@model IEnumerable<OfficeFileManagementSystem.Models.IncomingFile>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FileName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date_Allowed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FileType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Importance_ID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FileName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date_Allowed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FileType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Importance_ID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Incoming_File_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Incoming_File_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Incoming_File_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
