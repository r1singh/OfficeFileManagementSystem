@model IEnumerable<OfficeFileManagementSystem.Models.Assignment>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Employee_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date_Allowed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Incoming_File_Id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Employee_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date_Allowed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Incoming_File_Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Assignment_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Assignment_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Assignment_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
